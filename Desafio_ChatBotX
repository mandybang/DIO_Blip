const comando = gets(); // LÃª o comando enviado ao chatbot

switch (comando) {
    case "horario":
        print("Atendemos das 8h as 18h");
        break;

    case "telefone":
        print("Nosso telefone e 4002-8922");
        break;

    case "servicos":
        print("Oferecemos suporte, vendas e consultoria");
        break;

    case "sair":
        print("Encerrando atendimento");
        break;

    default:
        print("Comando invalido");
        break;
}
